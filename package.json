{
  "name": "node_starter",
  "version": "1.0.0",
  "description": "this is startkit for setting up enviroment with lint and nodeman",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "start": "nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnjneyaP/node_starter.git"
  },
  "author": "Anjneya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnjneyaP/node_starter/issues"
  },
  "homepage": "https://github.com/AnjneyaP/node_starter#readme",
  "dependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  }
}
